    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "less": "^4.1.2",
    "prettier": "^2.5.1",
    "sass": "^1.35.2",
    "typescript": "^4.5.4",
    "vite": "^2.8.4",
    "vite-plugin-cdn-import": "^0.3.5",
    "vite-plugin-compression": "^0.3.3",
