    "nprogress": "^0.2.0",
    "pinia": "^2.0.0-rc.10",
    "qs": "^6.10.1",
    "three": "^0.136.0",
    "vite-plugin-eruda": "^1.0.1",
    "vue": "^3.2.23",
    "vue-router": "^4.0.11",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.170",
    "@types/node": "^16.0.0",
    "@types/three": "^0.136.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
