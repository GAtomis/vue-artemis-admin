{
  "name": "vue-artemis-admin",
  "version": "1.0.0",
  "author": "GAtomis(Gavin)",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint:eslint": "eslint {library,src}/**/*.{vue,ts,tsx} --fix",
